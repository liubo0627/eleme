<%-include ../public/navbar%>

<%-include ../public/nav%>


<div class="content">

    <div class="container-fluid">
        <div class="row-fluid">

         <!--这是新闻列表-->

            <a href="<%-host%>admin/news_cata/add">添加新闻列表</a><br/>

            <table width="1000" border="1" cellpadding="0" cellspacing="0" align="center">
                <tr>
                    <th>类别编号</th>
                    <th>名称</th>
                    <th>时间</th>
                    <th>状态</th>
                    <th>操作</th>
                </tr>
                <%for(var i=0;i<list.length;i++) {%>
                <tr align="center">
                    <td><%=list[i].cate_id%></td>
                    <td><%=list[i].cate_name%></td>
                    <td><%=list[i].add_time%></td>
                    <td><%=list[i].status%></td>
                    <td><a href="<%-host%>admin/news_cata/edit?id=<%=list[i]._id%>">修改</a>
                        <a href="<%-host%>admin/news_cata/delete?id=<%=list[i]._id%>">删除</a>
                    </td>
                </tr>
                <%}%>
            </table>


        </div>
    </div>
</div>



<script src="<%=host%>lib/bootstrap/js/bootstrap.js"></script>
<script type="text/javascript">
    $("[rel=tooltip]").tooltip();
    $(function() {
        $('.demo-cancel-click').click(function(){return false;});
    });
</script>

</body>
</html>


