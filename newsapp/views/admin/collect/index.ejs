<style>
    .list_content{
        width:300px;
        overflow: hidden;
        white-space: nowrap;
        text-overflow: ellipsis;
    }
</style>


<%-include ../public/navbar%>

<%-include ../public/nav%>


<div class="content">

    <div class="container-fluid">
        <div class="row-fluid">

         <h2>用户收藏管理页面</h2>
            <a href="<%-host%>admin/collect/add">添加新闻</a><br/>

            <table width="90%" border="1" cellpadding="0" cellspacing="0" align="center">
                <tr>
                    <th>用户名</th>
                    <th>类别编号</th>
                    <th>新闻编号</th>
                    <th>添加时间</th>
                    <th>操作</th>
                </tr>
                <%for(var i=0;i<list.length;i++) {%>
                    <tr align="center">
                        <td><%=list[i].username%></td>
                        <td><%=list[i].cate_id%></td>
                        <td><%=list[i].title%></td>
                        <td><%=list[i].add_time%></td>
                        <td><a href="<%-host%>admin/collect/edit?id=<%=list[i]._id%>">修改</a>
                            <a href="<%-host%>admin/collect/delete?id=<%=list[i]._id%>">删除</a>
                        </td>
                    </tr>
                <%}%>
            </table>




        </div>
    </div>
</div>



<script src="<%=host%>lib/bootstrap/js/bootstrap.js"></script>
<script type="text/javascript">
    $("[rel=tooltip]").tooltip();
    $(function() {
        $('.demo-cancel-click').click(function(){return false;});
    });
</script>

</body>
</html>


